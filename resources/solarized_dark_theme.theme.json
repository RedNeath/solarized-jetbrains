{
  "name": "Solarized Dark",
  "dark": true,
  "author": "Tyler Thrailkill",
  "editorScheme": "/Solarized Dark.xml",
  "ui": {
    "Borders": {
      "color": "#073642",
      "ContrastBorderColor": "#073642"
    },
    "*": {
      "background": "#002b36",
      "foreground": "#839496",
      "selectionBackground": "#073642",
      "selectionForeground": "#839496"
    },
    "*.darcula": {
      "background": "#002b36",
      "foreground": "#839496",
      "selectionBackground": "#073642",
      "selectionForeground": "#839496"
    },

    "Label.foreground": "#586e75",
    "Label.background": "#002b36",
    "Label.selectedForeground": "#586e75",
    "FileColor": {
      "Yellow": "#b58900",
      "Green": "#859900",
      "Blue": "#268bd2",
      "Violet": "#6c71c4",
      "Orange": "#cb4b16",
      "Rose": "#dc322f"
    },

    "ActionButton": {
      "hoverBorderColor": "#002b36",
      "pressedBorderColor": "#002b36"
    },
    "Button.darcula": {
      "startColor": "#002b36",
      "endColor": "#002b36",
      "outlineStartColor": "#073642",
      "outlineEndColor": "#073642",
      "focusedOutlineColor": "#93a1a1",
      "defaultStartColor": "#93a1a1",
      "defaultEndColor": "#93a1a1",
      "defaultOutlineColor": "#93a1a1",
      "outlineDefaultStartColor": "#93a1a1",
      "outlineDefaultEndColor": "#93a1a1",
      "defaultFocusedOutlineColor": "#002b36",
      "selectedButtonForeground": "#002b36",
      "smallComboButtonBackground": "#002b36"
    },

    "ComboBox.darcula": {
      "nonEditableBackground": "#002b36",
      "arrowButtonBackground": "#002b36",
      "editable.arrowButtonBackground": "#002b36",
      "arrowButtonForeground": "#002b368b",
      "arrowButtonDisabledForeground": "#002b36b1"
    },

    "Component.grayForeground": "#586e75",

    "Editor": {
      "background": "#002b36",
      "foreground": "#8394968b",
      "shortcutForeground": "#268bd2"
    },

    "EditorPane.inactiveBackground": "#002b36",

    "EditorTabs": {
      "active": {
        "foreground": "#839496",
        "background": "#073642",
        "underlineColor": "#93a1a1"
      },
      "inactive.maskColor": "#073642FF"
    },

    "FlameGraph": {
      "JVMBackground": "#FFD333",
      "JVMSearchNotMatchedBackground": "#806919",
      "nativeBackground": "#93a1a1",
      "nativeSearchNotMatchedBackground":"#00838F",
      "parentBackground":"#002b368b",
      "parentSearchNotMatchedBackground": "#757575",

      "JVMFocusBackground":"#073642",
      "JVMFocusSearchNotMatchedBackground": "#073642",
      "nativeFocusBackground": "#073642",
      "nativeFocusSearchNotMatchedBackground":"#073642",
      "parentFocusBackground":"#073642",
      "parentFocusSearchNotMatchedBackground": "#073642",

      "JVMFrameForeground":"#000000",
      "nativeFrameForeground":"#000000",
      "parentFrameForeground":"#000000",

      "JVMFocusedFrameForeground": "#002b368b",
      "nativeFocusedFrameForeground": "#002b368b",
      "parentFocusedFrameForeground": "#002b368b"
    },

    "Focus": {
      "borderColor": "#93a1a1",
      "defaultButtonBorderColor": "#93a1a1"
    },

    "Group.separatorColor": "#073642",

    "Menu": {
      "separatorColor": "#073642"
    },

    "NavBar.borderColor": "#073642",

    "Separator.foreground": "#073642",

    "SidePanel.background": "#002b36",

    "StatusBar.borderColor": "#073642",

    "Table.stripeColor": "#073642d3",
    "Table.selectionBackground": "#268bd2aa",
    "Table.selectionInactiveBackground": "#268bd2FF",
    "Table.selectionForeground": "#000000",
    "Table.focusCellForeground": "#000000",
    
    "ToggleButton": {
      "on.foreground":  "#002b368b",
      "off.foreground": "#002b368b",
      "buttonColor":  "#002b368b",
      "borderColor": "#002b368b"
    },

    "ToolWindow": {
      "header": {
        "background": "#002b36",
        "border.background": "#073642",
        "tab.selected": {
          "active.background": "#586e75FF",
          "background": "#002b36FF"
        },
        "tab.hovered": {
          "active.background": "#586e75FF",
          "background": "#002b36FF"
        }
      },
      "Button": {
        "hoverBackground": "#073642",
        "selectedBackground": "#073642",
        "selectedForeground": "#839496"
      }
    },
    "ToolWindow.darcula": {
      "header": {
        "background": "#002b36",
        "border.background": "#073642",
        "tab.selected": {
          "active.background": "#586e75FF",
          "background": "#002b36FF"
        },
        "tab.hovered": {
          "active.background": "#586e75FF",
          "background": "#002b36FF"
        }
      },
      "Button": {
        "hoverBackground": "#073642",
        "selectedBackground": "#073642",
        "selectedForeground": "#839496"
      }
    }
  }
}
