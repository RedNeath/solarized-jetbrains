{
  "name": "Solarized <color>",
  "dark": "<dark>",
  "author": "Tyler Thrailkill",
  "editorScheme": "/themes/solarized<color>.xml",
  "ui": {
    "Borders": {
      "color": "#<background_highlights>",
      "ContrastBorderColor": "#<background_highlights>"
    },
    "*": {
      "background": "#<background>",
      "foreground": "#<primary_text>",
      "selectionBackground": "#<background_highlights>",
      "selectionForeground": "#<primary_text>"
    },
    "*.darcula": {
      "background": "#<background>",
      "foreground": "#<primary_text>",
      "selectionBackground": "#<background_highlights>",
      "selectionForeground": "#<primary_text>"
    },

    "Label.foreground": "#<secondary_text>",
    "Label.background": "#<background>",
    "Label.selectedForeground": "#<secondary_text>",
    "FileColor": {
      "Yellow": "#b58900",
      "Green": "#859900",
      "Blue": "#268bd2",
      "Violet": "#6c71c4",
      "Orange": "#cb4b16",
      "Rose": "#dc322f"
    },

    "ActionButton": {
      "hoverBorderColor": "#<background>",
      "pressedBorderColor": "#<background>"
    },
    "Button": {
      "startBackground": "#<background>",
      "endBackground": "#<background>",
      "startBorderColor": "#<background_highlights>",
      "endBorderColor": "#<background_highlights>",
      "focusedBorderColor": "#<emphasized_content>",
      "default": {
        "startBackground": "#<emphasized_content>",
        "endBackground": "#<emphasized_content>",
        "startBorderColor": "#<emphasized_content>",
        "endBorderColor": "#<emphasized_content>",
        "focusedBorderColor": "#<background>",
        "foreground": "#<background>"
      }
    },

    "ComboBox": {
      "nonEditableBackground": "#<background>",
      "arrowButtonBackground": "#<background>",
      "editable.arrowButtonBackground": "#<background>",
      "arrowButtonForeground": "#<background>8b",
      "arrowButtonDisabledForeground": "#<background>b1"
    },

    "Component.grayForeground": "#<secondary_text>",

    "Editor": {
      "background": "#<background>",
      "foreground": "#<primary_text>8b",
      "shortcutForeground": "#268bd2"
    },

    "EditorPane.inactiveBackground": "#<background>",

    "EditorTabs": {
      "active": {
        "foreground": "#<primary_text>",
        "background": "#<background_highlights>",
        "selectedBackground": "#<background>",
        "underlineColor": "#<emphasized_content>"
      },
      "inactive.maskColor": "#<background_highlights>FF"
    },

    "FlameGraph": {
      "JVMBackground": "#FFD333",
      "JVMSearchNotMatchedBackground": "#806919",
      "nativeBackground": "#<emphasized_content>",
      "nativeSearchNotMatchedBackground": "#00838F",
      "parentBackground": "#<background>8b",
      "parentSearchNotMatchedBackground": "#757575",
      "JVMFocusBackground": "#<background_highlights>",
      "JVMFocusSearchNotMatchedBackground": "#<background_highlights>",
      "nativeFocusBackground": "#<background_highlights>",
      "nativeFocusSearchNotMatchedBackground": "#<background_highlights>",
      "parentFocusBackground": "#<background_highlights>",
      "parentFocusSearchNotMatchedBackground": "#<background_highlights>",
      "JVMFrameForeground": "#000000",
      "nativeFrameForeground": "#000000",
      "parentFrameForeground": "#000000",
      "JVMFocusedFrameForeground": "#<background>8b",
      "nativeFocusedFrameForeground": "#<background>8b",
      "parentFocusedFrameForeground": "#<background>8b"
    },

    "Focus": {
      "borderColor": "#<emphasized_content>",
      "defaultButtonBorderColor": "#<emphasized_content>"
    },

    "Group.separatorColor": "#<background_highlights>",

    "Menu": {
      "separatorColor": "#<background_highlights>"
    },

    "NavBar.borderColor": "#<background_highlights>",

    "Separator.foreground": "#<background_highlights>",

    "SidePanel.background": "#<background>",

    "StatusBar.borderColor": "#<background_highlights>",

    "Table.stripeColor": "#<background_highlights>d3",
    "Table.selectionBackground": "#268bd2aa",
    "Table.selectionInactiveBackground": "#268bd2FF",
    "Table.selectionForeground": "#000000",
    "Table.focusCellForeground": "#000000",
    
    "ToggleButton": {
      "onForeground": "#<background>8b",
      "offForeground": "#<background>8b",
      "buttonColor": "#<background>8b",
      "borderColor": "#<background>8b"
    },
    
    "ToolWindow": {
      "Header": {
        "inactiveBackground": "#<background>",
        "background": "#<background>",
        "borderColor": "#<background_highlights>"
      },
      "HeaderTab": {
        "selectedBackground": "#<secondary_text>FF",
        "selectedInactiveBackground": "#<background>FF",
        "hoverBackground": "#<secondary_text>FF",
        "hoverInactiveBackground": "#<background>FF"
      },
      "Button": {
        "hoverBackground": "#<background_highlights>",
        "selectedBackground": "#<background_highlights>",
        "selectedForeground": "#<primary_text>"
      }
    }
  }
}
