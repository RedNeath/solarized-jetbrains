{
  "name": "Solarized Light",
  "dark": false,
  "author": "Tyler Thrailkill",
  "editorScheme": "/themes/solarizedLight.xml",
  "ui": {
    "Borders": {
      "color": "#eee8d5",
      "ContrastBorderColor": "#eee8d5"
    },
    "*": {
      "background": "#fdf6e3",
      "foreground": "#657b83",
      "selectionBackground": "#eee8d5",
      "selectionForeground": "#657b83"
    },
    "*.darcula": {
      "background": "#fdf6e3",
      "foreground": "#657b83",
      "selectionBackground": "#eee8d5",
      "selectionForeground": "#657b83"
    },

    "Label.foreground": "#93a1a1",
    "Label.background": "#fdf6e3",
    "Label.selectedForeground": "#93a1a1",
    "FileColor": {
      "Yellow": "#b58900",
      "Green": "#859900",
      "Blue": "#268bd2",
      "Violet": "#6c71c4",
      "Orange": "#cb4b16",
      "Rose": "#dc322f"
    },

    "ActionButton": {
      "hoverBorderColor": "#fdf6e3",
      "pressedBorderColor": "#fdf6e3"
    },
    "Button": {
      "startBackground": "#fdf6e3",
      "endBackground": "#fdf6e3",
      "startBorderColor": "#eee8d5",
      "endBorderColor": "#eee8d5",
      "focusedBorderColor": "#586e75",
      "default": {
        "startBackground": "#586e75",
        "endBackground": "#586e75",
        "startBorderColor": "#586e75",
        "endBorderColor": "#586e75",
        "focusedBorderColor": "#fdf6e3",
        "foreground": "#fdf6e3"
      }
    },

    "ComboBox": {
      "nonEditableBackground": "#fdf6e3",
      "arrowButtonBackground": "#fdf6e3",
      "editable.arrowButtonBackground": "#fdf6e3",
      "arrowButtonForeground": "#fdf6e38b",
      "arrowButtonDisabledForeground": "#fdf6e3b1"
    },

    "Component.grayForeground": "#93a1a1",

    "Editor": {
      "background": "#fdf6e3",
      "foreground": "#657b838b",
      "shortcutForeground": "#268bd2"
    },

    "EditorPane.inactiveBackground": "#fdf6e3",

    "EditorTabs": {
      "active": {
        "foreground": "#657b83",
        "background": "#eee8d5",
        "selectedBackground": "#fdf6e3",
        "underlineColor": "#586e75"
      },
      "inactive.maskColor": "#eee8d5FF"
    },

    "FlameGraph": {
      "JVMBackground": "#FFD333",
      "JVMSearchNotMatchedBackground": "#806919",
      "nativeBackground": "#586e75",
      "nativeSearchNotMatchedBackground": "#00838F",
      "parentBackground": "#fdf6e38b",
      "parentSearchNotMatchedBackground": "#757575",
      "JVMFocusBackground": "#eee8d5",
      "JVMFocusSearchNotMatchedBackground": "#eee8d5",
      "nativeFocusBackground": "#eee8d5",
      "nativeFocusSearchNotMatchedBackground": "#eee8d5",
      "parentFocusBackground": "#eee8d5",
      "parentFocusSearchNotMatchedBackground": "#eee8d5",
      "JVMFrameForeground": "#000000",
      "nativeFrameForeground": "#000000",
      "parentFrameForeground": "#000000",
      "JVMFocusedFrameForeground": "#fdf6e38b",
      "nativeFocusedFrameForeground": "#fdf6e38b",
      "parentFocusedFrameForeground": "#fdf6e38b"
    },

    "Focus": {
      "borderColor": "#586e75",
      "defaultButtonBorderColor": "#586e75"
    },

    "Group.separatorColor": "#eee8d5",

    "Menu": {
      "separatorColor": "#eee8d5"
    },

    "NavBar.borderColor": "#eee8d5",

    "Separator.foreground": "#eee8d5",

    "SidePanel.background": "#fdf6e3",

    "StatusBar.borderColor": "#eee8d5",

    "Table.stripeColor": "#eee8d5d3",
    "Table.selectionBackground": "#268bd2aa",
    "Table.selectionInactiveBackground": "#268bd2FF",
    "Table.selectionForeground": "#000000",
    "Table.focusCellForeground": "#000000",
    
    "ToggleButton": {
      "onForeground": "#fdf6e38b",
      "offForeground": "#fdf6e38b",
      "buttonColor": "#fdf6e38b",
      "borderColor": "#fdf6e38b"
    },
    
    "ToolWindow": {
      "Header": {
        "inactiveBackground": "#fdf6e3",
        "background": "#fdf6e3",
        "borderColor": "#eee8d5"
      },
      "HeaderTab": {
        "selectedBackground": "#93a1a1FF",
        "selectedInactiveBackground": "#fdf6e3FF",
        "hoverBackground": "#93a1a1FF",
        "hoverInactiveBackground": "#fdf6e3FF"
      },
      "Button": {
        "hoverBackground": "#eee8d5",
        "selectedBackground": "#eee8d5",
        "selectedForeground": "#657b83"
      }
    }
  }
}
